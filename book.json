{
    "gitbook": "3.x.x",
    "language": "bn",
    "pdf": {
        "headerTemplate": "ওয়ার্ডপ্রেস সিকিউরিটি - _SECTION_",
        "footerTemplate": "_AUTHOR_ / _PAGENUM_"
    },
    "styles": {
        "pdf": "styles/pdf.css"
    },
    "plugins": [
        "pdf-styling",
        "packt-fontsettings"
    ],
    "pluginsConfig": {
        "packt-fontsettings": {
            "theme": "white",
            "family": "Solaimanlipi",
            "size": "2"
        }
    }
}